CREATE TABLE register (
	personId int   NOT NULL AUTO_INCREMENT,
    fullName varchar(40) NOT NULL,
    pass varchar(50),
    email varchar(40) ,
    roleId INT,
    ngo_name varchar(40) ,
    mobile_no varchar(15) ,
    address varchar(100),
    year_establishment INT,
     PRIMARY KEY (personId)
);


ALTER USER 'root'@'localhost' IDENTIFIED WITH mysql_native_password BY 'root';
flush privileges;
CREATE TABLE donator (
	personId int   NOT NULL AUTO_INCREMENT,
    brand_name varchar(40) NOT NULL,
    generic_name varchar(50),
    ngo_name varchar(40),
    medicine_type int,
    exp_date  date ,
    mobile_no varchar(15) ,
    quentity int(2),
    year_establishment INT,
    assign varchar(40),
    allow_status boolean,
    assign_executor varchar(40)
     
);


donator

personId, brand_name, generic_name, ngo_name, medicine_type, quantity, mobile_no, assign, allow_status, assign_executor, is_collected, donator_name, excutor_email, donator_address, donation_id, exp_date, ngo_email, donator_email


request
personId, brand_name, generic_name, ngo_name, quantity, mobile_no, assign, allow_status, assign_executor, recepient_name, excutor_email, recepient_adress, request_id, ngo_email, recepient_email